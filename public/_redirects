# Cloudflare Pages redirects
# https://developers.cloudflare.com/pages/configuration/redirects/

# Admin redirect only
/admin /admin/ 200

# Brand routes with Chinese character support
/brands/:slug /brands/:slug/ 200

# API routes (if any)
/api/* /api/:splat 200

# Studio uses default directory routing - /studio maps to /studio/index.html automatically
# No explicit studio redirects to avoid infinite loops